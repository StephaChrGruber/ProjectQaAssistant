# Backend from inside the docker network:
BACKEND_BASE_URL=http://backend:8080

# Public URL for browser calls if you use it in client code (optional, but safe)
NEXT_PUBLIC_BACKEND_BASE_URL=http://backend:8080
NEXT_PUBLIC_DEV_USER=stephan

# POC identity (used by Next.js server routes to send headers)
POC_DEV_USER=stephan@local
POC_DEV_ADMIN=true

# NextAuth not used in POC; keep dummy if still present
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=dev-not-used

# Entra not used in POC; keep dummy
ENTRA_CLIENT_ID=dev
ENTRA_CLIENT_SECRET=dev
ENTRA_TENANT_ID=dev